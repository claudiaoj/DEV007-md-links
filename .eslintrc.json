{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off"
    },
    "plugins": ["import"],
    "overrides": [
    {
    "files": ["**/*.spec.js", "**/*.spec.jsx"]
    }
]
}
